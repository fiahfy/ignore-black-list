{
  "name": "Ignore Black List",
  "version": "1.0.0",
  "description": "Ignore sites included black list with searching on Google.",

  "manifest_version": 2,

  "icons": {
  },

  "browser_action": {
    "default_popup": "html/popup.html"
  },

  "background": {
    "page": "html/background.html"
  },

  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://www.google.co.jp/"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
